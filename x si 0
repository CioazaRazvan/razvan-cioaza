        <script>
          let pushes = 1;
          let winner = 0;

          function checkButtons() {
              const btn1 = document.getElementById('btn1');
              const btn2 = document.getElementById('btn2');
              const btn3 = document.getElementById('btn3');
              const btn4 = document.getElementById('btn4');
              const btn5 = document.getElementById('btn5');
              const btn6 = document.getElementById('btn6');
              const btn7 = document.getElementById('btn7');
              const btn8 = document.getElementById('btn8');
              const btn9 = document.getElementById('btn9');

              if (btn1.textContent == 'x' && btn1.textContent == btn5.textContent && btn1.textContent == btn9.textContent) {
                  alert("Jucatorul care a pus " + btn1.textContent + " este castigator");
                  ++winner;
              } else if (btn7.textContent == 'x' && btn7.textContent == btn5.textContent && btn7.textContent == btn3.textContent) {
                  alert("Jucatorul care a pus " + btn7.textContent + " este castigator");
                  ++winner;
              } else if (btn1.textContent == 'x' && btn1.textContent == btn2.textContent && btn1.textContent == btn3.textContent) {
                  alert("Jucatorul care a pus " + btn1.textContent + " este castigator");
                  ++winner;
              } else if (btn4.textContent == 'x' && btn4.textContent == btn5.textContent && btn4.textContent == btn6.textContent) {
                  alert("Jucatorul care a pus " + btn4.textContent + " este castigator");
                  ++winner;
