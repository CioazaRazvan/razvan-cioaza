                  alert("Jucatorul care a pus " + btn2.textContent + " este castigator");
                  ++winner;
              } else if (btn3.textContent == 'x' && btn3.textContent == btn6.textContent && btn3.textContent == btn9.textContent) {
                  alert("Jucatorul care a pus " + btn3.textContent + " este castigator");
                  ++winner;
              } else if (btn1.textContent == '0' && btn1.textContent == btn5.textContent && btn1.textContent == btn9.textContent) {
                  alert("Jucatorul care a pus " + btn1.textContent + " este castigator");
                  ++winner;
              } else if (btn7.textContent == '0' && btn7.textContent == btn5.textContent && btn7.textContent == btn3.textContent) {
                  alert("Jucatorul care a pus " + btn7.textContent + " este castigator");
                  ++winner;
