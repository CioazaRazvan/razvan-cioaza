                  alert("Jucatorul care a pus " + btn3.textContent + " este castigator");
                  ++winner;
              }
          }

          function pickButton1() {
              const btn = document.getElementById('btn1');
              if (btn.textContent != '0' && btn.textContent != 'x') {
                  if (pushes % 2 == 0) {
                      btn.textContent = '0';
                  } else {
                      btn.textContent = 'x';
                  }
                  ++pushes;
              }
              checkButtons();
              if (pushes == 10) {
                  if (winner == 0) {
                      alert("Este egalitate");
                  }
              }
          }

          function pickButton2() {
