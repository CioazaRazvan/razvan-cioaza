                  ++winner;
              } else if (btn4.textContent == '0' && btn4.textContent == btn5.textContent && btn4.textContent == btn6.textContent) {
                  alert("Jucatorul care a pus " + btn4.textContent + " este castigator");
                  ++winner;
              } else if (btn7.textContent == '0' && btn7.textContent == btn8.textContent && btn7.textContent == btn9.textContent) {
